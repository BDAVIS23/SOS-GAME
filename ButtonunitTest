class Test_test_Changeturneven(unittest.TestCase):
    def test_changeturn(self):
        SOS_GameGrid_sprint2.RB=0
        turnvalue= SOS_GameGrid_sprint2.RB % 2
        self.assertEqual(turnvalue,0)

class Test_test_Changeturnodds(unittest.TestCase):
    def test_changeturn(self):
        SOS_GameGrid_sprint2.RB=1
        turnvalue= SOS_GameGrid_sprint2.RB % 2
        self.assertEqual(turnvalue,1)



class Test_test_gameselectsimple(unittest.TestCase):
    def test_gameselect(self):
        SOS_GameGrid_sprint2.Gametype=0
        gametype= SOS_GameGrid_sprint2.Gametype
        self.assertEqual(gametype,0)


class Test_test_gameselectgeneral(unittest.TestCase):
    def test_gameselect(self):
        SOS_GameGrid_sprint2.Gametype=1
        gametype= SOS_GameGrid_sprint2.Gametype
        self.assertEqual(gametype,1)





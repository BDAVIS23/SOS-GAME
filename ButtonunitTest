class Test_test_button0S(unittest.TestCase):
    def test_Update_SOB0(self):
       SOS_GameGrid_sprint2.ButtonChange0= 'S'
      
       buttonchange0 = SOS_GameGrid_sprint2.ButtonChange0
       
       self.assertEqual(buttonchange0,'S')
       
      
